{
  "name": "chakra-ui-headroom",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build-fast": "turbo run build-fast --filter=./packages/**/*",
    "build": "turbo run build --filter=./packages/**/*",
    "start": "turbo run start --filter=./packages/**/*",
    "prepare": "husky install",
    "swc": "node -r @swc-node/register",
    "clean-pkgs": "pnpm -r exec rm -rf dist .turbo .swc *.log",
    "clean": "pnpm clean-pkgs && rm -rf node_modules",
    "dev:next": "pnpm --filter \"./examples/next-ts\" dev",
    "typecheck": "turbo run typecheck --filter=./packages/**/*"
  },
  "dependencies": {
    "husky": "^8.0.1",
    "tsup": "6.2.3",
    "typescript": "4.8.3"
  },
  "license": "MIT",
  "keywords": [],
  "description": "",
  "devDependencies": {
    "turbo": "1.4.7"
  }
}
